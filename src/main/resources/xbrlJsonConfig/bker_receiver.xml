    <instanceDoc>
        <nameSpaceRef>
            <nameSpace nameSpacePrefix = "xsi" nameSpaceURI = "http://www.w3.org/2001/XMLSchema-instance" />
            <nameSpace nameSpacePrefix = "bker" nameSpaceURI = "http://xbrl.mof.gov.cn/taxonomy/2023-12-31/bker" />
            <nameSpace nameSpacePrefix = "link" nameSpaceURI = "http://www.xbrl.org/2003/linkbase" />
