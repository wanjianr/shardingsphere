        <SchemaLocationRef value ="http://xbrl.org/2006/xbrldi http://www.xbrl.org/2006/xbrldi-2006.xsd" />
        <UnitRef>
            <unit id ="u1" measure ="iso4217:CNY" numerator ="" denominator =""/>
